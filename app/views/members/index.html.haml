- title "Members"

%table
  %tr
    %th First name
    %th Last name
    %th Status
  - for member in @members
    %tr
      %td= member.first_name
      %td= member.last_name
      %td= member.status
      %td= link_to 'Show', member
      %td= link_to 'Edit', edit_member_path(member)
      %td= link_to 'Destroy', member, :confirm => 'Are you sure?', :method => :delete

%p= link_to "New Member", new_member_path
